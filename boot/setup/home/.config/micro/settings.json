{
    "autoclose": true,
    "autoindent": true,
    "autosave": 0,
    "autosu": false,
    "backup": true,
    "basename": false,
    "clipboard": "terminal",
    "colorcolumn": 0,
    "colorscheme": "default",
    "comment": true,
    "cursorline": true,
    "diff": true,
    "diffgutter": false,
    "encoding": "utf-8",
    "eofnewline": true,
    "fastdirty": false,
    "fileformat": "unix",
    "filemanager": true,
    "filemanager.compressparent": true,
    "filemanager.foldersfirst": true,
    "filemanager.openonstart": false,
    "filemanager.showdotfiles": true,
    "filemanager.showignored": true,
    "filetype": "unknown",
    "ftoptions": true,
    "fzf": true,
    "ignorecase": false,
    "indentchar": " ",
    "infobar": true,
    "joinLines": true,
    "keepautoindent": false,
    "keymenu": false,
    "linter": true,
    "literate": true,
    "matchbrace": true,
    "mkparents": true,
    "mouse": false,
    "parsecursor": false,
    "paste": false,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "quoter": true,
    "quoter.enable": true,
    "readonly": false,
    "relativeruler": false,
    "rmtrailingws": true,
    "ruler": true,
    "savecursor": false,
    "savehistory": true,
    "saveundo": false,
    "scrollbar": false,
    "scrollmargin": 3,
    "scrollspeed": 2,
    "smartpaste": true,
    "softwrap": false,
    "splitbottom": true,
    "splitright": true,
    "status": true,
    "statusformatl": "$(filename) $(modified)(line: $(line), col: $(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "statusformatr": "$(bind:command-edit:help ): Help Topics",
    "statusline": true,
    "sucmd": "sudo",
    "syntax": true,
    "tabmovement": false,
    "tabsize": 4,
    "tabstospaces": false,
    "useprimary": true,
    "xterm": true,
    "ft:elixir": {
        "tabsize": 2
    },
    "ft:ruby": {
        "tabsize": 2
    }
}
